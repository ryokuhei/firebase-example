# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Firebase-example' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Shine_for_iOS
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'GoogleSignIn'
  pod 'RxCocoa'
  pod 'RxSwift'

  target 'Firebase-exampleTests' do
    inherit! :search_paths
    # Pods for testing

    pod 'Firebase'
  end

  target 'Firebase-exampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
